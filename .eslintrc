{
    "env": {
      "es6": true,
      "node": true,
      "jest/globals": true
    },
    "extends": ["eslint:recommended", "plugin:node/recommended", "airbnb", "plugin:jest/recommended", "plugin:jest/style", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "jest/no-disabled-tests": "warn",
      "jest/no-focused-tests": "error",
      "jest/no-identical-title": "error",
      "jest/prefer-to-have-length": "warn",
      "jest/valid-expect": "error"
    },
    "plugins": ["jest"]
  }
  