{
  "name": "rb-toolbox",
  "version": "1.0.0",
  "description": "A javascript utilities package.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "devSetup": "curl https://get.volta.sh | bash && npm ci && husky install",
    "start": "node index.js",
    "start:dev": "node --watch index.js",
    "precommit": "eslint .",
    "lint": "eslint src/**/*.{js}",
    "lint:fix": "eslint - fix src/**/*.{js}",
    "format": "prettier - write src/**/*.{js} - config .prettierrc.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolling-bones/rb-toolbox.git"
  },
  "keywords": [
    "utilities",
    "javascript",
    "js",
    "tools",
    "toolbox",
    "utils"
  ],
  "author": "rolling-bones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rolling-bones/rb-toolbox/issues"
  },
  "homepage": "https://github.com/rolling-bones/rb-toolbox#readme",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-require-extensions": "^0.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3"
  },
  "volta": {
    "node": "21.0.0",
    "npm": "10.2.1"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
